<header>
  <div class="container-fluid">
    <div class="row">
      <nav class="navbar navbar-expand-lg navbar-light w-100">
        <div class="col-12 d-flex flex-wrap">
          <% if admin_signed_in? %>
            <%= render "layouts/headers/admin"%>
          <% elsif users_signed_in? %>
            <%= render "layouts/headers/user"%>
          <% else %>
            <%= render "layouts/headers/public" %>
          <% end %>
        </div>
      </nav>
    </div>
  </div>
</header>